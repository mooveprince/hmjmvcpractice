steal('can', '<%= appPath %>/models/<%= underscore %>.js', './init.ejs', 'jquery/dom/form_params',
	function (can, <%= Alias %>, initEJS) {

	/**
	 * @constructor <%= appPath %>/<%= underscore %>/create
	 * @alias <%=Alias%>Create
	 * @parent <%= appName %>
	 * @inherits can.Control
	 * Creates <%= plural %>
	 */
	return can.Control(
	/** @Prototype */
	{
		/**
		 *  Render the initial template
		 */
		init: function () {
			this.element.html(initEJS());
		},
		/**
		 *  Submit handler. Create a new <%= underscore %> from the form.
		 */
		submit: function (el, ev) {
			ev.preventDefault();
			el.find('[type=submit]').val('Creating...')
			new <%= Alias %>(el.formParams()).save(function() {
				el.find('[type=submit]').val('Create');
				el[0].reset()
			});
		}
	});
});